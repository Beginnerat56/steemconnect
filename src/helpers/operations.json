{
  "transfer": {
    "name": "Transfer",
    "description": "Transfers asset from one account to another.",
    "schema": {
      "from": {
        "type": "account",
        "default": "__signer"
      },
      "to": {
        "type": "account"
      },
      "amount": {
        "type": "amount"
      },
      "memo": {
        "type": "string",
        "default": ""
      }
    }
  },
  "delegate_vesting_shares": {
    "name": "Delegate Steem Power",
    "schema": {
      "delegator": {
        "type": "account",
        "default": "__signer"
      },
      "delegatee": {
        "type": "account"
      },
      "vesting_shares": {
        "type": "amount"
      }
    }
  },
  "transfer_to_vesting": {
    "name": "Power up",
    "schema": {
      "from": {
        "type": "account",
        "default": "__signer"
      },
      "to": {
        "type": "account"
      },
      "amount": {
        "type": "amount"
      }
    }
  },
  "withdraw_vesting": {
    "name": "Power down",
    "schema": {
      "account": {
        "type": "account",
        "default": "__signer"
      },
      "vesting_shares": {
        "type": "amount"
      }
    }
  },
  "convert": {
    "schema": {
      "owner": {
        "type": "account",
        "default": "__signer"
      },
      "requestid": {
        "type": "int"
      },
      "amount": {
        "type": "amount"
      }
    }
  },
  "account_witness_vote": {
    "name": "Witness vote",
    "schema": {
      "account": {
        "type": "account",
        "default": "__signer"
      },
      "witness": {
        "type": "account"
      },
      "approve": {
        "type": "bool",
        "default": true
      }
    }
  },
  "claim_account": {
    "schema": {
      "creator": {
        "type": "account",
        "default": "__signer"
      },
      "fee": {
        "type": "amount",
        "default": "0.000 STEEM"
      },
      "extensions": {
        "type": "array",
        "default": []
      }
    }
  }
}
